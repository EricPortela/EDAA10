import java.util.ArrayList;

public class TestSortAlgorithms {

	public static void main(String[] args) {
		
		
		//Selection sort with ArrayList<Int>
		
		SelectionSort ss = new SelectionSort(); //Create object
		
		ArrayList<Integer> unsortedArray = ss.generateRandomInt(20, 1, 50); //Generate random ArrayList
				
		ArrayList<Integer> sortedArray = ss.selectionSortInt(unsortedArray, true); //Sort in descending order
		
		System.out.println(sortedArray);

		
		InsertionSort is = new InsetionSort();
		
		
	}

}
